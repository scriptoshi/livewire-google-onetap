{
    "name": "scriptoshi/livewire-google-onetap",
    "description": "A Laravel Livewire component for Google One Tap authentication",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Scriptoshi",
            "email": "author@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/auth": "^12.0",
        "livewire/livewire": "^4.0",
        "laravel/socialite": "^5.0",
        "google/apiclient": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "Scriptoshi\\LivewireGoogleOneTap\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Scriptoshi\\LivewireGoogleOneTap\\GoogleOneTapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}